{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "@electron-toolkit/eslint-config-ts/recommended",
    "@electron-toolkit/eslint-config-prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "printWidth": 200
      }
    ],
    "max-len": [
      "warn",
      {
        "code": 200
      }
    ]
  },
  "settings": {
//    "import/resolver": {
//      "typescript": {}
//    }
  }
}
